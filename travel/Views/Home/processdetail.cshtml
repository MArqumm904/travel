@model travel.Models.Processtb

@{
    ViewData["Title"] = "processdetail";
    Layout = "adminlayout";

}




<div class="container">
    <div class="container">
        <div style="width:100%; height:100px; background-color:transparent;">
        </div>
        <h1>Process Detail</h1>

        <div>

            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.ProcessName)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.ProcessName)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.ProcessDescription)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.ProcessDescription)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.ProcessImage)
                </dt>
                <dd class="col-sm-10">
                    <img src="~/images/@Html.DisplayFor(model => model.ProcessImage)" style="height:100px; width:100px;" />

                </dd>
            </dl>
            <hr />
        </div>
        <div>
            <a asp-action="editpro" asp-route-id="@Model?.ProcessId">Edit</a> |
            <a asp-action="processlist">Back to List</a>
        </div>
    </div>
</div>
